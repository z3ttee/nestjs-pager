{
  "name": "nestjs-pager",
  "version": "1.1.0",
  "description": "A simple Pagination implementation for NestJS applications.",
  "scripts": {
    "package-publish": "npm run build && npm version patch --no-git-tag-version && npm publish",
    "build": "rimraf dist && tsc --project tsconfig.esm.json & tsc --project tsconfig.json"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.0.0",
    "@nestjs/common": "^9.0.3",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^9.0.3",
    "@nestjs/platform-express": "^9.0.3",
    "@nestjs/schematics": "^9.0.1",
    "@nestjs/typeorm": "^9.0.0",
    "@types/express": "^4.17.13",
    "@types/node": "^18.0.5",
    "@typescript-eslint/eslint-plugin": "^5.30.6",
    "@typescript-eslint/parser": "^5.30.6",
    "eslint": "^8.20.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.5.6",
    "ts-loader": "^9.3.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.0.0",
    "typeorm": "^0.3.7",
    "typescript": "^4.7.4"
  },
  "peerDependencies": {
    "@nestjs/common": "^9.0.0",
    "@nestjs/core": "^9.0.0"
  },
  "keywords": [
    "nestjs",
    "pagination"
  ],
  "author": "z3ttee",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/z3ttee/nestjs-pager.git"
  },
  "homepage": "https://github.com/z3ttee/nestjs-pager",
  "types": "./dist/types/index.d.ts",
  "files": [
    "dist/**/*"
  ],
  "exports": {
    "import": "./dist/esm/index.js",
    "require": "./dist/cjs/index.js",
    "default": "./dist/esm/index.js"
  }
}
